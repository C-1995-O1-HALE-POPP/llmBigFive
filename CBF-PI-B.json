[
  {"id": 1,  "text": "我常感到害怕",                                   "dimension": "N", "reverse": false},
  {"id": 2,  "text": "一旦确定了目标，我会坚持努力地实现它",             "dimension": "C", "reverse": false},
  {"id": 3,  "text": "我觉得大部分人基本上是心怀善意的",                 "dimension": "A", "reverse": false},
  {"id": 4,  "text": "我头脑中经常充满生动的画面",                       "dimension": "O", "reverse": false},
  {"id": 5,  "text": "我对人多的聚会感到乏味",                           "dimension": "E", "reverse": true},
  {"id": 6,  "text": "有时我觉得自己一无是处",                           "dimension": "N", "reverse": false},
  {"id": 7,  "text": "我常常是仔细考虑之后才做出决定",                   "dimension": "C", "reverse": false},
  {"id": 8,  "text": "我不太关心别人是否受到不公正的待遇",               "dimension": "A", "reverse": true},
  {"id": 9,  "text": "我是个勇于冒险，突破常规的人",                     "dimension": "O", "reverse": false},
  {"id": 10, "text": "在热闹的聚会上，我常常表现主动并尽情玩耍",         "dimension": "E", "reverse": false},
  {"id": 11, "text": "别人一句漫不经心的话，我常会联系在自己身上",       "dimension": "N", "reverse": false},
  {"id": 12, "text": "别人认为我是个慎重的人",                           "dimension": "C", "reverse": false},
  {"id": 13, "text": "我时常觉得别人的痛苦与我无关",                     "dimension": "A", "reverse": true},
  {"id": 14, "text": "我喜欢冒险",                                       "dimension": "O", "reverse": false},
  {"id": 15, "text": "我尽量避免参加人多的聚会和嘈杂的环境",             "dimension": "E", "reverse": true},
  {"id": 16, "text": "在面对压力时，我有种快要崩溃的感觉",               "dimension": "N", "reverse": false},
  {"id": 17, "text": "我喜欢一开头就把事情计划好",                       "dimension": "C", "reverse": false},
  {"id": 18, "text": "我是那种只照顾好自己，不替别人担忧的人",           "dimension": "A", "reverse": true},
  {"id": 19, "text": "我对许多事情有着很强的好奇心",                     "dimension": "O", "reverse": false},
  {"id": 20, "text": "有我在的场合一般不会冷场",                         "dimension": "E", "reverse": false},
  {"id": 21, "text": "我常担忧一些无关紧要的事情",                       "dimension": "N", "reverse": false},
  {"id": 22, "text": "我工作或学习很勤奋",                               "dimension": "C", "reverse": false},
  {"id": 23, "text": "虽然社会上有些骗子，但我觉得大部分人还是可信的",   "dimension": "A", "reverse": false},
  {"id": 24, "text": "我身上具有别人没有的冒险精神",                     "dimension": "O", "reverse": false},
  {"id": 25, "text": "在一个团体中，我希望处于领导地位",                 "dimension": "E", "reverse": false},
  {"id": 26, "text": "我常常感到内心不踏实",                             "dimension": "N", "reverse": false},
  {"id": 27, "text": "我是个倾尽全力做事的人",                           "dimension": "C", "reverse": false},
  {"id": 28, "text": "当别人向我诉说不幸时，我常感到难过",               "dimension": "A", "reverse": false},
  {"id": 29, "text": "我渴望学习一些新东西，即使它们与我的日常生活无关", "dimension": "O", "reverse": false},
  {"id": 30, "text": "别人多认为我是一个热情和友好的人",                 "dimension": "E", "reverse": false},
  {"id": 31, "text": "我常担心有什么不好的事情要发生",                   "dimension": "N", "reverse": false},
  {"id": 32, "text": "在工作上，我常只求能应付过去便可",                 "dimension": "C", "reverse": true},
  {"id": 33, "text": "尽管社会存在阴暗面，我仍然相信人性总体善良",       "dimension": "A", "reverse": false},
  {"id": 34, "text": "我的想象力相当丰富",                               "dimension": "O", "reverse": false},
  {"id": 35, "text": "我喜欢参加社交与娱乐聚会",                         "dimension": "E", "reverse": false},
  {"id": 36, "text": "我很少感到忧郁或沮丧",                             "dimension": "N", "reverse": true},
  {"id": 37, "text": "做事讲究逻辑和条理是我的一个特点",                 "dimension": "C", "reverse": false},
  {"id": 38, "text": "我常为那些遭遇不幸的人感到难过",                   "dimension": "A", "reverse": false},
  {"id": 39, "text": "我很愿意也很容易接受新事物、新观点、新想法",       "dimension": "O", "reverse": false},
  {"id": 40, "text": "我希望成为领导者而不是被领导者",                   "dimension": "E", "reverse": false}
]
